{
	"name": "logicbrush/silverstripe-rolluppage",
	"description": "A module for the SilverStripe CMS which rolls up the contents of child pages for display on a single page.",
	"homepage": "https://www.logicbrush.com/specialities/silverstripe-development/",
	"type": "silverstripe-vendormodule",
	"keywords": [
		"silverstripe",
		"rolluppage"
	],
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "John Learn",
		"email": "jflearn@logicbrush.com"
	}],
	"require": {
		"php": "^7.4||^8.0",
		"silverstripe/cms": "^4.0"
	},
	"extra": {
		"expose": [
			"css",
			"images",
			"javascript"
		]
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"silverstripe/sqlite3": "^2.2"
	},
	"autoload": {
		"psr-4": {
			"Logicbrush\\RollupPage\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Logicbrush\\RollupPage\\Tests\\": "tests/"
		}
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"silverstripe/vendor-plugin": true
		}
	}
}
