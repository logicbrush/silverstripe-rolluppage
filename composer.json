{
  "name": "logicbrush/silverstripe-rolluppage",
  "description": "A module for the SilverStripe CMS which rolls up the contents of child pages for display on a single page.",
  "homepage": "https://github.com/logicbrush/silverstripe-rolluppage",
  "keywords": ["silverstripe"],
  "license": "BSD-3-Clause",
  "authors": [
    {"name": "John Learn","email": "jflearn@logicbrush.com"}
  ],
  "support": {
    "issues": "http://github.com/logicbrush/silverstripe-rollupage/issues"
  },
  "require": {
    "silverstripe/cms": "^4",
    "silverstripe/framework": "^4"
  },
  "autoload": {
    "psr-4": {
        "Logicbrush\\RollupPage\\": "src/"
    }
  },
  "extra": {
    "installer-name": "silverstripe-rolluppage",
    "expose": [
        "client"
    ],
    "screenshots": [
      "relative/path/screenshot1.png",
      "http://myhost.com/screenshot2.png"
    ]
  }
}
